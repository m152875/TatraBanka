            <param name="id">ID dropdown listu.</param>
            <param name="gridActionOnItemSelected">Akcia v gride, ktorú má vyvolať výber položky.</param>
            <param name="toolTip">Text tooltipu.</param>
            <param name="items">Pole položiek. Musí mať nastavené platné Value a Text.</param>
            <returns>Dropdown list zaradený do toolbaru.</returns>
        </member>
